{
    "OwnerDetails": {
        "UserId": "",
        "PlatformId": ""
    },
    "Auth": {
        "Authority": "",
        "ApiName": "api_player"
    },
    "Cors": {
        "Hosts": []
    },
    "Elasticsearch": {
        "Uri": "http://localhost:9200",
        "Username": "platform_user",
        "Password": ""
    },
    "Serilog": {
        "Elasticsearch": {
            "Enabled": true,
            "AutoRegisterTemplate": true,
            "IndexFormat": "platform-missing-{0:yyyy.MM}"
        },
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "Enrich": [
            "WithThreadId"
        ],
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "[{Timestamp:MM-dd-yyyy HH:mm:ss.fffffffzzz}] [{ThreadId}] [{ServiceName}] [{Level}] [{SourceContext}] {Message}{NewLine}{Exception}"
                }
            }
        ]
    },
    "Logging": {
        "IncludeScopes": false,
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    }
}